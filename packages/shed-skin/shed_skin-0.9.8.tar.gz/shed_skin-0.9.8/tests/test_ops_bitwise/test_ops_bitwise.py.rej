--- tests/test_ops_bitwise/test_ops_bitwise.py
+++ tests/test_ops_bitwise/test_ops_bitwise.py
@@ -6,22 +6,22 @@ def test_bitwise_operators():
     b = 13            # 13 = 0000 1101 
     c = 0
 
-    c = a & b;        # 12 = 0000 1100
+    c = a & b        # 12 = 0000 1100
     assert c == 12
 
-    c = a | b;        # 61 = 0011 1101
+    c = a | b        # 61 = 0011 1101
     assert c == 61
 
-    c = a ^ b;        # 49 = 0011 0001
+    c = a ^ b        # 49 = 0011 0001
     assert c == 49
 
-    c = ~a;           # -61 = 1100 0011
+    c = ~a           # -61 = 1100 0011
     assert c == -61
 
-    c = a << 2;       # 240 = 1111 0000
+    c = a << 2       # 240 = 1111 0000
     assert c == 240
 
-    c = a >> 2;       # 15 = 0000 1111
+    c = a >> 2       # 15 = 0000 1111
     assert c == 15
 
 
